<p>
  Logged in as <strong><%= current_user.email %></strong>.</br>
<%= link_to 'Edit your profile', edit_user_registration_path %>
</p>

<div class="mx-auto w-25">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">First name</th>
        <th scope="col">Last name</th>
      </tr>
    </thead>
    <tbody>
      <% @followees.find_each do |followee| %>
        <tr id="followee">
          <td><%= followee.first_name  %></td>
          <td><%= followee.last_name  %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
